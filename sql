SELECT CASE WHEN process_list.status_id = 5 THEN (select desk_id from process_list_hist where process_type = {process_type_id} and ref_id = {app_id} order by `id` desc limit 1 offset 1) WHEN process_list.status_id = 22 THEN 1 ELSE 0 END as process_starting_desk from (select status_id from process_list where process_type_id = {process_type_id} and ref_id = {app_id}) as process_list
